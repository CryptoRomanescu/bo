# H-5N1P3R Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Solana RPC endpoint URL (use a private RPC endpoint for production)
RPC_URL=https://api.mainnet-beta.solana.com

# Wallet public key for transaction monitoring
WALLET_PUBKEY=11111111111111111111111111111112

# API Keys (if using external services)
# DISCORD_BOT_TOKEN=your_discord_bot_token_here
# TWITTER_API_KEY=your_twitter_api_key_here
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Smart Money Tracking API Keys
# NANSEN_API_KEY=your_nansen_api_key_here
# BIRDEYE_API_KEY=your_birdeye_api_key_here

# Smart Money Configuration
# SMART_MONEY_DETECTION_WINDOW_SECS=60
# SMART_MONEY_MIN_WALLETS=2
# SMART_MONEY_ENABLE_NANSEN=false
# SMART_MONEY_ENABLE_BIRDEYE=true

# Metrics endpoint authentication (generate a secure random token)
# METRICS_AUTH_TOKEN=your_secure_random_token_here

# Database encryption key (optional, for future use)
# DB_ENCRYPTION_KEY=your_encryption_key_here
